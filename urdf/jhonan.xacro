<?xml version='1.0'?>

<robot name="jhonan" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find jhonan)/urdf/jhonan.gazebo" />
  <!-- argumentos -->
  <xacro:arg name="base_frame"    default="chassis" />
  

  <gazebo reference="left_wheel">
    <material>Gazebo/Green</material>
  </gazebo>

  <gazebo reference="right_wheel">
    <material>Gazebo/Green</material>
  </gazebo>
<!--
  <gazebo reference="camera">
    <material>Gazebo/Red</material>
  </gazebo>-->

  <gazebo reference="$(arg base_frame)">
    <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="camera_depth_optical">
    <material>Gazebo/black</material>
  </gazebo>

  <gazebo reference="left_camera_optical">
    <material>Gazebo/black</material>
  </gazebo>



  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="$(arg base_frame)" />
  </joint>
<!--
  <joint name="camera_joint" type="fixed">
    <origin xyz="0.26 0 0" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="camera" />  
    <axis xyz="0 1 0"/> 
  </joint>-->
<!--
  <joint name="hokuyo_joint" type="fixed">
    <origin xyz="0 0 0.2" rpy="0 0 0" />
    <parent link="$(arg base_frame)"/>
    <child link="hokuyo" />  
    <axis xyz="0 1 0"/> 
  </joint>
-->
  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.30 0" rpy="0 0 0" />
    <parent link="$(arg base_frame)"/>
    <child link="left_wheel" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="10000" velocity="1000" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.30 0" rpy="0 0 0" />
    <parent link="$(arg base_frame)"/>
    <child link="right_wheel" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="10000" velocity="1000" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <joint name="camera_depth_frame_joint" type="fixed">
     <origin xyz="0 0 0.35" rpy="0 0 0" />
     <parent link="$(arg base_frame)" />
     <child link="camera_depth_frame" />
  </joint>

  <joint name="left_camera_frame_joint" type="fixed">
     <origin xyz="0 0 0.45" rpy="0 0 0" />
     <parent link="$(arg base_frame)" />
     <child link="left_camera_frame" />
  </joint>



